{
  "name": "go-kubebuilder",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "kubebuilder",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": [],
      "options": {
        "command": "make build",
        "cwd": "apps/go/kubebuilder"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "outputs": [],
      "options": {
        "command": "make test",
        "cwd": "apps/go/kubebuilder"
      }
    },
    "serve": {
      "executor": "@nx-go/nx-go:serve",
      "options": {
        "main": "apps/go/kubebuilder/main.go"
      }
    },
    "lint": {
      "executor": "@nx-go/nx-go:lint"
    },
    "docker": {
      "executor": "@nrwl/workspace:run-commands",
      "outputs": [],
      "options": {
        "parallel": false,
        "commands": [
          "make docker-build",
          "make docker-push"
        ],
        "cwd": "apps/go/kubebuilder"
      }
    }
  },
  "tags": []
}
